<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="at.ac.fh.salzburg.MeterManagementEntity" table="meter_management" schema="SmartValDB">
        <composite-id mapped="true" class="at.ac.fh.salzburg.MeterManagementEntityPK">
            <key-property name="idMeter" column="id_meter"/>
            <key-property name="idType" column="id_type"/>
            <key-property name="idManufactor" column="id_manufactor"/>
            <key-property name="idProtocol" column="id_protocol"/>
            <key-property name="idCustomer" column="id_customer"/>
        </composite-id>
        <property name="description" column="description"/>
        <property name="serial" column="serial"/>
        <property name="key" column="key"/>
        <property name="period" column="period"/>
        <property name="active" column="active"/>
        <property name="port" column="port"/>
    </class>
</hibernate-mapping>