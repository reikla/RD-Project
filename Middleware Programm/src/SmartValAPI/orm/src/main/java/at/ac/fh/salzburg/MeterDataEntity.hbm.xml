<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="at.ac.fh.salzburg.MeterDataEntity" table="meter_data" schema="SmartValDB">
        <composite-id mapped="true" class="at.ac.fh.salzburg.MeterDataEntityPK">
            <key-property name="dataId" column="data_id"/>
            <key-property name="meterId" column="meter_id"/>
        </composite-id>
        <property name="timestamp" column="timestamp"/>
        <property name="countTotal" column="count_total"/>
        <property name="countRegister1" column="count_register1"/>
        <property name="countRegister2" column="count_register2"/>
        <property name="countRegister3" column="count_register3"/>
        <property name="countRegister4" column="count_register4"/>
        <property name="powerP1" column="power_p1"/>
        <property name="powerP2" column="power_p2"/>
        <property name="powerP3" column="power_p3"/>
        <property name="workP1" column="work_p1"/>
        <property name="workP2" column="work_p2"/>
        <property name="workP3" column="work_p3"/>
        <property name="frequency" column="frequency"/>
        <property name="voltage" column="voltage"/>
    </class>
</hibernate-mapping>