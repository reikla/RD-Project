<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="at.ac.fh.salzburg.LogEventsEntity" table="log_events" schema="SmartValDB">
        <composite-id mapped="true" class="at.ac.fh.salzburg.LogEventsEntityPK">
            <key-property name="logId" column="log_id"/>
            <key-property name="idType" column="id_type"/>
        </composite-id>
        <property name="content" column="content"/>
        <property name="sourceTarget" column="source_target"/>
        <property name="timestamp" column="timestamp"/>
    </class>
</hibernate-mapping>